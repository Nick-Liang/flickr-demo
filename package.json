{
  "name": "Flickr-Demo",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "build": "rimraf dist && webpack --progress",
    "build.prod": "npm run build && browserify -s main dist/main.bundle.js > dist/bundle.js && npm run minify",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --progress --port 3000 --content-base src",
    "start": "npm run server"
  },
  "main": "main.ts",
  "contributors": [
    "Nick Liang <nick.liang.au@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~7.2.14",
    "@angular/compiler": "~7.2.14",
    "@angular/compiler-cli": "~7.2.14",
    "@angular/core": "~7.2.14",
    "@angular/forms": "~7.2.14",
    "@angular/http": "~7.2.14",
    "@angular/platform-browser": "~7.2.14",
    "@angular/platform-browser-dynamic": "~7.2.14",
    "@angular/platform-server": "~7.2.14",
    "@angular/animations": "^7.2.14",
    "@angular/router": "~7.2.14",
    "@angular/upgrade": "~7.2.14",
    "angular-in-memory-web-api": "^0.8.0",
    "browserify": "^16.2.3",
    "core-js": "^2.6.5",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "clean-webpack-plugin": "^2.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "raw-loader": "^2.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "to-string-loader": "^1.1.5",
    "typescript": "~3.2.4",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "yarn": "^1.15.2"
  },
  "keywords": [
    "Angular2",
    "Flickr Demo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nick-Liang/flickr-demo"
  },
  "bugs": {},
  "homepage": "https://github.com/Nick-Liang"
}
